{
	"name": "react-spreadsheet-upload",
	"version": "1.0.5",
	"description": "Simple React component to upload spreadsheets",
	"author": "Jo√£o Pedro <contato.gepetojj@gmail.com>",
	"license": "MIT",
	"main": "dist/rsu.umd.js",
	"keywords": [
		"react",
		"spreadsheet",
		"upload",
		"import",
		"csv",
		"xlsx",
		"validate"
	],
	"homepage": "https://github.com/gepetojj/react-spreadsheet-upload",
	"bugs": {
		"url": "https://github.com/gepetojj/react-spreadsheet-upload/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gepetojj/react-spreadsheet-upload.git"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		"./styles.css": "./dist/styles.css",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/rsu.es.js",
			"require": "./dist/rsu.umd.js"
		}
	},
	"scripts": {
		"build": "tsc && vite build && pnpm build:css",
		"build:css": "pnpx @tailwindcss/cli -m -i ./src/tw.css -o ./dist/styles.css",
		"ci": "pnpm build",
		"lint": "biome lint --write .",
		"format": "biome format --write .",
		"commit": "git cz",
		"test": "vitest --run",
		"test:watch": "vitest",
		"preview": "vite preview",
		"prepublishOnly": "pnpm run ci",
		"storybook": "concurrently \"pnpm storybook:css\" \"storybook dev -p 6006\"",
		"storybook:css": "pnpx @tailwindcss/cli -w -i ./src/tw.css -o ./src/styles.css",
		"build-storybook": "concurrently \"pnpm build-storybook:css\" \"storybook build\"",
		"build-storybook:css": "pnpx @tailwindcss/cli -m -i ./src/tw.css -o ./dist/styles.css",
		"analyze": "pnpm build && npx vite-bundle-analyzer dist/rsu.es.js",
		"size": "pnpm build && du -sh dist/* && echo 'Bundle sizes:' && ls -lah dist/*.js"
	},
	"dependencies": {
		"clsx": "2.1.1",
		"papaparse": "5.5.3",
		"xlsx": "0.18.5"
	},
	"peerDependencies": {
		"react": "^18 || ^19",
		"react-dom": "^18 || ^19"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.4",
		"@chromatic-com/storybook": "^4.1.1",
		"@storybook/addon-a11y": "^9.1.7",
		"@storybook/addon-docs": "^9.1.7",
		"@storybook/addon-onboarding": "^9.1.7",
		"@storybook/addon-vitest": "^9.1.7",
		"@storybook/react-vite": "^9.1.7",
		"@tailwindcss/cli": "^4.1.13",
		"@tailwindcss/vite": "^4.1.13",
		"@types/papaparse": "^5.3.16",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.3",
		"@vitest/browser": "^3.2.4",
		"@vitest/coverage-v8": "^3.2.4",
		"commitizen": "^4.3.1",
		"concurrently": "^9.2.1",
		"cz-conventional-changelog": "^3.3.0",
		"playwright": "^1.55.0",
		"storybook": "^9.1.7",
		"tailwindcss": "^4.1.13",
		"terser": "^5.36.0",
		"typescript": "^5.5.4",
		"vite": "^7.1.6",
		"vite-bundle-analyzer": "^1.2.3",
		"vite-plugin-dts": "^4.5.4",
		"vitest": "^3.2.4"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
