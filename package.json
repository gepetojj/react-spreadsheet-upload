{
	"name": "react-spreadsheet-upload",
	"version": "1.0.0",
	"description": "Simple React component to upload spreadsheets",
	"author": "Jo√£o Pedro <contato.gepetojj@gmail.com>",
	"license": "MIT",
	"main": "dist/index.jsx",
	"keywords": [
		"react",
		"spreadsheet",
		"upload",
		"import",
		"csv",
		"xlsx",
		"validate"
	],
	"homepage": "https://github.com/gepetojj/react-spreadsheet-upload",
	"bugs": {
		"url": "https://github.com/gepetojj/react-spreadsheet-upload/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gepetojj/react-spreadsheet-upload.git"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.jsx",
			"default": "./dist/index.jsx"
		}
	},
	"packageManager": "pnpm@10.16.1",
	"scripts": {
		"ci": "pnpm build && pnpm check-exports",
		"build": "tsup",
		"lint": "biome lint --write .",
		"format": "biome format --write .",
		"test": "vitest --run",
		"test:watch": "vitest",
		"commit": "git cz",
		"check-exports": "attw --pack .",
		"local-release": "changeset version && changeset publish",
		"prepublishOnly": "pnpm run ci",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"react": "^19.1.1",
		"react-dom": "^19.1.1"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.18.2",
		"@biomejs/biome": "^2.1.4",
		"@changesets/cli": "^2.29.5",
		"@chromatic-com/storybook": "^4.1.1",
		"@storybook/addon-a11y": "^9.1.7",
		"@storybook/addon-docs": "^9.1.7",
		"@storybook/addon-onboarding": "^9.1.7",
		"@storybook/addon-vitest": "^9.1.7",
		"@storybook/react-vite": "^9.1.7",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"storybook": "^9.1.7",
		"tsup": "^8.5.0",
		"typescript": "^5.5.4",
		"vitest": "^3.2.4",
		"@vitest/browser": "^3.2.4",
		"playwright": "^1.55.0",
		"@vitest/coverage-v8": "^3.2.4"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
